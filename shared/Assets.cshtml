@inherits Custom.Hybrid.Razor14
@using ToSic.Razor.Blade;

@{
    var data = new {
        domAttribute = DynamicModel.DomAttribute,
        currentCategory = CmsContext.Page.Parameters["category"]
    };
}
@* Include bootstrap 5 if the framework isn't known - then this file is still called, and we add Bootstrap to the page *@
@Kit.Page.Activate(condition: Kit.Css.IsUnknown, features: "Bootstrap5")

@Kit.Page.TurnOn("window.appJobs2.init()", data: data)

<link rel="stylesheet" href='@App.Path/dist/@(Kit.Css.Is("bs3") ? "bs3" : Kit.Css.Is("bs4") ? "bs4" : "bs5").min.css' @Kit.Page.AssetAttributes()>
<script src="@App.Path/dist/scripts.min.js" @Kit.Page.AssetAttributes()></script>